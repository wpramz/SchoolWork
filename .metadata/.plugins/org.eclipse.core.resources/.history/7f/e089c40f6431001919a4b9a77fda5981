#include "main.h"
using namespace std;

int main(int argc, char *argv[]) {
	try{

		clock_t t1,t2;
		string projFile;
		int size;
		if(argc<2){
			cout<<"There are insufficient arguments"<<endl;
			throw "Insufficient Arguments";
		}
		projFile = argv[1];
		ifstream inFile;
		inFile.open(projFile.c_str());
		inFile>>size;
		int list[size];
		for(int i=0; i< size; i++){
			inFile>>list[i];
		}
		t1 = clock();
		orderedList listOne(size, list);
		t1 = clock();
		printf("It took me %f seconds.\n",t1,((float)t1)/CLOCKS_PER_SEC);
		t2 = clock();
		mtfList listTwo(size, list);
		t2 = clock();
		printf("It took me %f seconds.\n",t2,((float)t2)/CLOCKS_PER_SEC);
		cout<<"inside my main"<<endl;
	}
	catch(...){
		cout<<"Caught unknown error. Will Exit program now..."<<endl;
	}
	return 0;
}
